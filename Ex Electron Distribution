sum = int(input())
sum_two = 0

result_list = []
i = 0
while sum_two < sum:
    i += 1
    result = 2*i**2
    sum_two += result
    if sum_two > sum:
        result -= sum_two - sum
    result_list.append(result)

print(result_list)


